# Environment Configuration Example
# Copy this file to .env and update with your values

# Backend API URL
VITE_API_URL=http://localhost:5000

# Python Proctoring Service URL
VITE_PYTHON_SERVICE_URL=http://localhost:8000

# Environment (development, staging, production)
VITE_ENV=development

# Enable debug mode (shows console logs)
VITE_DEBUG=true

# Exam defaults (for testing)
VITE_DEFAULT_DURATION=60
VITE_AUTO_SAVE_INTERVAL=30000

# Proctoring settings
VITE_FRAME_CAPTURE_INTERVAL=5000
VITE_VIDEO_CHUNK_DURATION=30000

# Feature flags
VITE_ENABLE_WEBCAM=true
VITE_ENABLE_PROCTORING=true
VITE_ENABLE_AUTO_SAVE=true

# Note: In Vite, environment variables must be prefixed with VITE_
# They are exposed to the client-side code
# Do NOT put sensitive keys here (API keys, secrets, etc.)
